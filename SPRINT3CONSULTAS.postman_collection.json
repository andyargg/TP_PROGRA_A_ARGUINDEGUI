				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Response status code is 500\", function () {\r",
							"    pm.response.to.have.status(500);\r",
							"});\r",
							"\r",
							"\r",
							"pm.test(\"Response time is less than 200ms\", function () {\r",
							"  pm.expect(pm.response.responseTime).to.be.below(200);\r",
							"});\r",
							"\r",
							"\r",
							"pm.test(\"Content-Type header is present and has the value 'text/html'\", function () {\r",
							"    pm.expect(pm.response.headers.get(\"Content-Type\")).to.equal(\"text/html\");\r",
							"});\r",
							"\r",
							"\r",
							"pm.test(\"Validate the presence of specific error details in the response\", function () {\r",
							"    pm.expect(pm.response.text()).to.include('Undefined variable $email');\r",
							"    pm.expect(pm.response.text()).to.include('Integrity constraint violation: 1048 Column \\'email\\' cannot be null');\r",
							"});\r",
							"\r",
							"\r",
							"pm.test(\"Error message contains details about undefined variable and SQL integrity constraint violation\", function () {\r",
							"    const responseData = pm.response.text();\r",
							"\r",
							"    pm.expect(responseData).to.include(\"Undefined variable $email\");\r",
							"    pm.expect(responseData).to.include(\"SQLSTATE[23000]: Integrity constraint violation\");\r",
							"});\r",
							""
